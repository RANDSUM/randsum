{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": ["auto"],
  "parser": "@typescript-eslint/parser",
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "jest"
  ]
}
