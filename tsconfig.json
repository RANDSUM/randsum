{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "lib": ["es2015"],
    "module": "commonjs",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  }
}
