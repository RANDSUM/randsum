{
  "compilerOptions": {
    "rootDir": ".",
    "declaration": true,
    "outDir": "build",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es6"
  }
}
