{
  "name": "@randsum/5e",
  "version": "0.1.18",
  "description": "A flexible, type-safe dice roller for building 5e-compatible applications",
  "author": {
    "name": "Alex Jarvis",
    "url": "https://github.com/alxjrvs"
  },
  "license": "MIT",
  "homepage": "https://github.com/RANDSUM/randsum",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RANDSUM/randsum.git",
    "directory": "packages/5e"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "react-native": "./src/index.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npx bunup",
    "test": "bun test",
    "prepare": "bun run build",
    "prepack": "bun run build"
  },
  "dependencies": {
    "@randsum/dice": "workspace:^"
  },
  "devDependencies": {
    "@types/bun": "1.2.6",
    "bunup": "^0.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "dice",
    "roller",
    "rpg",
    "random",
    "typescript",
    "tabletop",
    "5th",
    "5e",
    "Dungeons",
    "Dragons"
  ]
}
