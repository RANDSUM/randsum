{
  "name": "@randsum/notation",
  "version": "0.1.36",
  "description": "A flexible, type-safe dice notation parser and validator for tabletop RPGs, board games, and game development",
  "private": false,
  "author": {
    "name": "Alex Jarvis",
    "url": "https://github.com/alxjrvs"
  },
  "license": "MIT",
  "homepage": "https://github.com/RANDSUM/randsum",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RANDSUM/randsum.git",
    "directory": "packages/notation"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "react-native": "./src/index.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "RANDSUM_DICE_NOTATION.md"
  ],
  "sideEffects": false,
  "type": "module",
  "dependencies": {
    "@randsum/core": "workspace:~"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "dice-notation",
    "parser",
    "validator",
    "rpg",
    "typescript",
    "dnd",
    "tabletop",
    "randsum",
    "dice-roller",
    "ttrpg",
    "dungeons-and-dragons",
    "pathfinder",
    "game-development",
    "board-games",
    "role-playing-games",
    "dice-parser",
    "dice-validator"
  ]
}