{
  "name": "@randsum/dice",
  "version": "1.0.0",
  "description": "A flexible, type-safe dice roller for TypeScript/JavaScript",
  "author": {
    "name": "Alex Jarvis",
    "url": "https://github.com/RANDSUM"
  },
  "license": "MIT",
  "homepage": "https://github.com/RANDSUM/randsum-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/RANDSUM/randsum-ts.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bunup",
    "clean": "rm -rf ./dist/ ./coverage/",
    "test": "bun test"
  },
  "dependencies": {
    "@randsum/core": "workspace:*",
    "@randsum/notation": "workspace:*",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bun": "1.2.6",
    "bunup": "^0.1.39"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
