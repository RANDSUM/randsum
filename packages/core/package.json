{
  "name": "@randsum/core",
  "version": "0.1.32",
  "description": "Internal utilities and types for @randsum packages",
  "private": false,
  "author": {
    "name": "Alex Jarvis",
    "url": "https://github.com/alxjrvs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RANDSUM/randsum.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "source": "./src/index.ts",
  "react-native": "./src/index.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "bunup --entry src/index.ts -o dist --format esm,cjs -m -d -s -t node -c",
    "test": "bun test",
    "lint": "eslint . -c ../../eslint.config.js",
    "lint:fix": "eslint . -c ../../eslint.config.js --fix",
    "format": "prettier . --check --ignore-path ../../.prettierignore",
    "format:fix": "prettier . --write --ignore-path ../../.prettierignore",
    "ts:check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  }
}