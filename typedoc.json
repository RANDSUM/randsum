{
  "$schema": "https://typedoc.org/schema.json",
  "tsconfig": "./tsconfig.typedoc.json",
  "entryPoints": [
    "packages/roller/src/index.ts",
    "packages/blades/src/index.ts",
    "packages/daggerheart/src/index.ts",
    "packages/fifth/src/index.ts",
    "packages/root-rpg/src/index.ts",
    "packages/salvageunion/src/index.ts",
    "packages/pbta/src/index.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "exclude": [
    "**/__tests__/**",
    "**/__benchmarks__/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.bench.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "readme": "packages/roller/README.md",
  "name": "RANDSUM API Documentation",
  "includeVersion": true,
  "gitRevision": "main",
  "navigationLinks": {
    "RANDSUM Home": "https://randsum.dev",
    "Docs": "https://randsum.dev/docs/",
    "GitHub": "https://github.com/RANDSUM/randsum"
  }
}
