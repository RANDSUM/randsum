{
  "name": "randsum",
  "description": "Random Numbers For Humans in JS (And Typescript!)",
  "license": "MIT",
  "author": "Alex Jarvis",
  "repository": {
    "type": "git",
    "url": "https://github.com/alxjrvs/randsum.git"
  },
  "version": "0.1.4",
  "main": "build/lib/index.js",
  "scripts": {
    "compile": "tsc -p .",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "jest",
    "watch-build": "tsc -w -p ."
  },
  "dependencies": {
    "log": "^1.4.0",
    "npm": "^6.9.0",
    "tslint": "^5.9.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "codecov": "^3.0.0",
    "ts-jest": "^24.0.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~3.2.2"
  },
  "keywords": [
    "chance",
    "dice",
    "dice rolls",
    "die",
    "die rolling",
    "rand",
    "rolling",
    "typescript"
  ],
  "engines": {
    "node": ">=6.2.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "rootDir": "lib/"
  }
}
