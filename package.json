{
  "name": "randsum",
  "version": "1.7.4",
  "license": "MIT",
  "author": "alxjrvs@gmail.com",
  "main": "dist/index.js",
  "module": "dist/randsum.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build && husky install",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "postinstall": "husky install",
    "prepublish": "dts build",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "postversion": "git push --no-verify && git push --tags --no-verify"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/randsum.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/randsum.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.6.0",
    "husky": "^8.0.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.ts": [
      "dts lint --fix"
    ]
  }
}
