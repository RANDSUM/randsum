{
  "name": "randsum",
  "version": "3.2.4",
  "private": false,
  "author": "Alex Jarvis",
  "icon": "https://raw.githubusercontent.com/RANDSUM/randsum-ts/main/icon.webp",
  "description": "Ergonomic Dice Rolling with Strong Typescript Support",
  "license": "MIT",
  "homepage": "https://github.com/RANDSUM/randsum-ts ",
  "keywords": [
    "dice",
    "d20",
    "dice roller",
    "rpg dice roller",
    "roll",
    "d&d",
    "dnd",
    "rpg",
    "ttrpg",
    "rpgs",
    "ttrpgs",
    "tabletop",
    "dungeons",
    "dragons",
    "roleplaying game",
    "rand",
    "random",
    "RNG",
    "notation",
    "typescript",
    "bun"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "files": [
    "./dist/index.js",
    "./dist/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RANDSUM/randsum-ts.git"
  },
  "sideEffects": false,
  "scripts": {
    "analyze": "size-limit",
    "build": "bun run clean && bun run build.ts",
    "clean": "rm -rf ./dist/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "prettier . --write",
    "format": "prettier . --check",
    "typeCheck": "tsc --noEmit",
    "prepublish": "bun run build",
    "postversion": "git push --no-verify && git push --tags --no-verify"
  },
  "prettier": {
    "endOfLine": "auto",
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "4 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "bun-plugin-dts": "^0.2.0",
    "bun-types": "latest",
    "eslint": "^8.41.0",
    "eslint-config-adjunct": "^4.12.2",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-auto": "^0.7.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-array-func": "^4.0.0",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-xss": "^0.1.12",
    "prettier": "^3.0.2",
    "size-limit": "^10.0.3",
    "typescript": "^5.2.2"
  },
  "type": "module"
}
