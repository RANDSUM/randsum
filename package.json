{
  "name": "randsum-monorepo",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ci": "bunx moon ci",
    "build": "bunx moon :build",
    "test": "bunx moon :test -u",
    "lint": "bunx moon :lint",
    "lint:fix": "bunx moon :lint -- --fix",
    "format": "bunx moon :format",
    "format:check": "bunx moon :formatCheck",
    "style:fix": "bunx moon :lint && bunx moon :format",
    "ts:check": "bunx moon :tsCheck"
  },
  "trustedDependencies": [
    "@moonrepo/cli"
  ],
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@moonrepo/cli": "1.38.6",
    "@types/bun": "1.2.19",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "bunup": "0.8.61",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.37.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.1.42"
}
